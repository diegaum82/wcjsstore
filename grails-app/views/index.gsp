		$(function(){
			// Calling Login Form
			$("#login_form").click(function(){
				$(".social_login").hide();
				$(".user_login").show();
				return false;
			});
	
			// Calling Register Form
			$("#register_form").click(function(){
				$(".social_login").hide();
				$(".user_register").show();
				$(".header_title").text('Register');
				return false;
			});
	
